            {
              "in": "body",
              "name": "coche",
              "schema": {
                "items": {
                  "$ref": "#/definitions/Coche"
                },
                "example": {
                  "marca": "Nissan",
                  "modelo": "Skyline R34"
                }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "{message: 'ok'}"
            }
          }
        }
      },
      "/coches/{id}": {
