          "parameters": [
            {
              "in": "path",
              "name": "id",
              "required": true,
              "type": "string"
            },
            {
              "in": "body",
              "name": "coche",
              "schema": {
                "items": {
                  "$ref": "#/definitions/Coche"
                },
                "example": {
                  "marca": "Nissan",
                  "modelo": "Skyline R34"
                }
              }
            }
          ],
          "responses": {
            "200": {
